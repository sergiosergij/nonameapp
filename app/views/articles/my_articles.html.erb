<h1> My articles </h1>
<table>
	<thread>
		<tr>
			<th>Title</th>
			<th>Body</th>
			<th colspan ="3"></th>
		</tr>
	</thread>

	<tbody>
		<% @articles.each do |article| %>
			<tr>
			<td><%= article.title %></td>
			<td><%= article.body %></td>
            <td><%= link_to "Show", article %></td>
			</tr>
		<% end %>
	</tbody>
</table>
<p><%= link_to "Create article", new_article_path %></p>
<p><%= link_to "All articles", articles_path %></p>
<br>