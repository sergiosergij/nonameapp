<h1>All mangas</h1>
 


<table>
	<thread>
		<tr>
			<th>Title</th>
			<th>Year</th>
            <th>Author</th>
			<th colspan ="3"></th>
		</tr>
	</thread>

	<tbody>
		<% @mangas.each do |manga| %>
			<tr>
			    <td><%= manga.title %></td>
			    <td><%= manga.year %></td>
                <td><%= manga.author %></td>
				<td>ID MANGA:<%= manga.id %></td>
				<td> || Are there file for this Manga? => <td>
				<td><%= manga.file.attached? %>||</td>
				<td> ID: <%= manga.file.id %>||</td>
				<td><%= manga.file %></td>
			</tr>
		<% end %>
	</tbody>
</table>
<br>
