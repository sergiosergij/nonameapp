
<h1> Create manga </h1>

<%= form_with model: @manga do |form| %>
    
    <div>
      <%= form.label :title %><br>
      <%= form.text_field :title %>
    </div>
  
    <div>
      <%= form.label :year %><br>
      <%= form.number_field :year%>
    </div>
  
    <div>
      <%= form.label :author %><br>
      <%= form.text_field :author%>
    </div>

    <div>
     <%= form.file_field :file%>
    </div>
    
    <div>
      <%= form.submit %>
    </div>
    
  <% end %>


